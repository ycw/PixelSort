{
  "name": "pixelsort",
  "version": "2.0.0",
  "author": "ycw (https://github.com/ycw)",
  "repository": "github:ycw/pixelsort",
  "description": "Pixelsort WASM impl written in AssemblyScript",
  "main": "index.js",
  "scripts": {
    "asbuild:untouched": "asc assembly/index.ts --target debug",
    "asbuild:optimized": "asc assembly/index.ts --target release",
    "asbuild": "npm run asbuild:untouched && npm run asbuild:optimized",
    "build": "npm run asbuild:optimized"
  },
  "devDependencies": {
    "assemblyscript": "^0.19.16"
  },
  "dependencies": {
    "@assemblyscript/loader": "^0.19.16"
  },
  "license": "MIT",
  "keywords": [
    "art",
    "wasm",
    "webassembly",
    "assemblyscript"
  ]
}
